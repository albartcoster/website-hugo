journal = {Journal of dairy science},
number = {11},
pages = {2905--2912},
publisher = {Elsevier},
title = {{{\{}P{\}}erformance of lactating dairy cows fed varying amounts of soyhulls as a replacement for corn grain}},
volume = {85},
year = {2002}
}
