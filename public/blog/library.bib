journal = {Journal of dairy science},
keywords = {Animal Nutritional Physiological Phenomena,Animal Nutritional Physiological Phenomena: physio,Animals,Cattle,Cattle: physiology,Colony Count, Microbial,Dairying,Diet,Dietary Proteins,Dietary Proteins: metabolism,Digestion,Digestion: drug effects,Female,Fermentation,Fermentation: drug effects,Hydrogen-Ion Concentration,Lactation,Lactation: drug effects,Lactose,Lactose: analysis,Medicago sativa,Milk,Milk Proteins,Milk Proteins: analysis,Milk: chemistry,Nitrogen,Nitrogen: analysis,Oils, Volatile,Oils, Volatile: administration {\&} dosage,Rumen,Rumen: drug effects,Rumen: metabolism,Rumen: microbiology,Silage,Urea,Urea: analysis,Zea mays},
