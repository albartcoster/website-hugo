@article{Shibata2009,
abstract = {In genetic mapping of complex traits, scored haplotypes are likely to represent only a subset of all causal polymorphisms. At the extreme of this scenario, observed polymorphisms are not themselves functional, and only linked to causal ones via linkage disequilibrium (LD). We will demonstrate that due to such incomplete knowledge regarding the underlying genetic mechanism, the variance of a trait may become different between the scored haplotypes. Thus, unequal variances between haplotypes may be indicative of additional functional polymorphisms affecting the trait. Methods accounting for such haplotype-specific variance may also provide an increased power to detect complex associations. We suggest ways to estimate and test these haplotypic variance contrasts, and incorporate them into the haplotypic tests for association. We further extend this approach to data with unknown gametic phase via likelihood-based simultaneous estimation of haplotypic effects and their frequencies. We find our approach to provide additional power, especially under the following types of models: (a) where scored and unobserved variants are epistatically interacting with each other; and (b) under heterogeneity models, where multiple unobserved mutations are linked to non-functional observed polymorphisms via LD. An illustrative example of usefulness of the method is discussed, utilizing analysis of multilocus effects within the catechol-O-methyltransferase gene.},
author = {Shibata, Kyoko and Diatchenko, Luda and Zaykin, Dmitri V},
doi = {10.1002/gepi.20358},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Shibata, Diatchenko, Zaykin - 2009 - Haplotype associations with quantitative traits in the presence of complex multilocus and heterogen.pdf:pdf},
issn = {0741-0395},
journal = {Genetic epidemiology},
keywords = {Adolescent,Adult,Alleles,Catechol O-Methyltransferase,Catechol O-Methyltransferase: genetics,Catechol O-Methyltransferase: physiology,Chromosome Mapping,Chromosome Mapping: statistics {\&} numerical data,Cohort Studies,Epistasis, Genetic,Female,Haplotypes,Humans,Likelihood Functions,Linkage Disequilibrium,Models, Genetic,Models, Statistical,Molecular Epidemiology,Pain Threshold,Pain Threshold: physiology,Polymorphism, Single Nucleotide,Quantitative Trait, Heritable,Young Adult},
month = {jan},
number = {1},
pages = {63--78},
pmid = {18636529},
title = {{Haplotype associations with quantitative traits in the presence of complex multilocus and heterogeneous effects.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2700858{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {33},
