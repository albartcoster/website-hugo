doi = {10.3168/jds.2009-2178},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Zebeli, Ametaj - 2009 - Relationships between rumen lipopolysaccharide and mediators of inflammatory response with milk fat production a.pdf:pdf},
isbn = {1525-3198 (Electronic) 0022-0302 (Linking)},
issn = {00220302},
journal = {Journal of Dairy Science},
keywords = {C-reactive protein,c-reactive protein,charide,dairy cow,milk fat depression,rumen lipopolysac-,rumen lipopolysaccharide},
