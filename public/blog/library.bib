file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Deelen et al. - 2014 - Evaluation of a Brix refractometer to estimate serum immunoglobulin G concentration in neonatal dairy calves.pdf:pdf},
issn = {1525-3198},
journal = {Journal of dairy science},
keywords = {immuno-,passive transfer,passive transfer,refractometer,immunoglobulin,calf,refractometer},
month = {jun},
number = {6},
pages = {3838--44},
pmid = {24704239},
publisher = {Elsevier},
title = {{Evaluation of a Brix refractometer to estimate serum immunoglobulin G concentration in neonatal dairy calves.}},
