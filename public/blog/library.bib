issn = {0022-0302},
journal = {Journal of dairy science},
keywords = {Animal Feed,Animals,Cattle,Cattle: physiology,Dietary Fats, Unsaturated,Digestion,Eating,Fats,Fats: analysis,Fatty Acids,Fatty Acids, Nonesterified,Fatty Acids, Nonesterified: blood,Fatty Acids, Omega-3,Fatty Acids, Omega-3: administration {\&} dosage,Fatty Acids: analysis,Female,Fish Oils,Formaldehyde,Formaldehyde: pharmacology,Linseed Oil,Linseed Oil: administration {\&} dosage,Linseed Oil: chemistry,Milk,Milk Proteins,Milk Proteins: analysis,Milk: chemistry,Milk: secretion,Ovarian Follicle,Ovarian Follicle: drug effects,Ovarian Follicle: physiology,Prostaglandins,Prostaglandins: secretion,Silage},
