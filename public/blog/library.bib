file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Meuwissen, Hayes, Goddard - 2001 - {\{}P{\}}rediction of total genetic value using genome-wide dense marker maps.pdf:pdf},
journal = {Genetics},
pages = {1819--1829},
title = {{{\{}P{\}}rediction of total genetic value using genome-wide dense marker maps}},
volume = {157},
year = {2001}
}
