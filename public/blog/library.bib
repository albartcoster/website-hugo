keywords = {Alleles,Computational Biology,DNA-Binding Proteins,DNA-Binding Proteins: genetics,Female,Gene Expression,Genomic Imprinting,Humans,Microfilament Proteins,Microfilament Proteins: genetics,Neoplasm Proteins,Neoplasm Proteins: genetics,Nerve Tissue Proteins,Nerve Tissue Proteins: genetics,Oligonucleotide Array Sequence Analysis,Placenta,Placenta: metabolism,Polymorphism, Single Nucleotide,Pregnancy,Sensitivity and Specificity},
month = {jan},
pages = {25},
pmid = {20403199},
title = {{High-throughput analysis of candidate imprinted genes and allele-specific gene expression in the human term placenta.}},
