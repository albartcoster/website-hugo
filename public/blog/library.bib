journal = {Journal of dairy science},
keywords = {Animals,Blood Chemical Analysis,Blood Chemical Analysis: veterinary,Blood Glucose,Body Weight,Cattle,Cattle: metabolism,Dairying,Diet,Diet: veterinary,Energy Intake,Energy Intake: physiology,Female,Insulin-Like Growth Factor I,Insulin-Like Growth Factor I: metabolism,Lactation,Lactation: metabolism,Liver,Liver: chemistry,Liver: metabolism,Milk,Milk: chemistry,Milk: secretion,Pregnancy,Regression Analysis,Thyroid Hormones,Thyroid Hormones: metabolism,Time Factors},
