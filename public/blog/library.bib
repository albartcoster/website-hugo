issn = {1525-3198},
journal = {Journal of dairy science},
keywords = {Animals,Breeding,Canada,Cattle,Cattle: genetics,Cattle: physiology,Female,Insemination, Artificial,Insemination, Artificial: veterinary,Male,Models, Statistical,Pregnancy,Quantitative Trait, Heritable,Reproduction,Reproduction: genetics,Seasons},
