doi = {10.1016/j.livprodsci.2005.08.003},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/De Campeneere, De Boever, De Brabander - 2006 - Comparison of rolled, NaOH treated and ensiled wheat grain in dairy cattle diets.pdf:pdf},
issn = {18711413},
journal = {Livestock Science},
keywords = {Digestibility,Ensiled immature wheat,Milking performance,NaOH treated wheat,Rolled wheat,Rumen degradability,sodagrain},
mendeley-tags = {sodagrain},
number = {2-3},
pages = {267--276},
title = {{Comparison of rolled, NaOH treated and ensiled wheat grain in dairy cattle diets}},
volume = {99},
year = {2006}
}
