@article{Hammon2006,
abstract = {The objectives of this study were to investigate the associations between peripheral blood neutrophil (PMN) function, energy status, and uterine health in periparturient dairy cows. Data were collected from 83 multiparous Holstein cows. Blood samples for PMN function determination were collected weekly from 1 week prior to calving (week -1) through 4 weeks after calving and again at 8 weeks after calving. Energy metabolites were measured and dry matter intake (DMI) was determined from weeks -2 to 5 to evaluate energy status of cows during the periparturient period. All cows were examined for uterine health disorders. Blood PMN killing ability was evaluated by determining myeloperoxidase activity and cytochrome c reduction activity in isolated blood PMN's. For cows that were diagnosed with puerperal metritis and subclinical (SC) endometritis and puerperal metritis, blood PMN functions were significantly (P {\textless} 0.05) impaired during the periparturient period, compared to cows with normal uterine health. Cows with subclinical endometritis and puerperal metritis or SC endometritis also had significantly (P {\textless} 0.01) higher NEFA and significantly (P {\textless} 0.001) lower DMI during the periparturient period, and significantly (P {\textless} 0.05) higher BHBA during early lactation, compared to cows with normal uterine health. Neutrophil function was also significantly (P {\textless} 0.01) impaired in cows with peripartum negative energy balance, which was characterized by elevated blood levels of NEFA and decreased DMI. Decreased PMN function and energy balance were associated with uterine health disorders and the decreases in PMN function and energy balance occurred prior to parturition and prior to the detection of these uterine disorders. ?? 2006 Elsevier B.V. All rights reserved.},
author = {Hammon, D. S. and Evjen, I. M. and Dhiman, T. R. and Goff, J. P. and Walters, J. L.},
doi = {10.1016/j.vetimm.2006.03.022},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Hammon et al. - 2006 - Neutrophil function and energy status in Holstein cows with uterine health disorders.pdf:pdf},
isbn = {0165-2427 (Print)$\backslash$r0165-2427 (Linking)},
issn = {01652427},
journal = {Veterinary Immunology and Immunopathology},
keywords = {Dairy cow,Endometritis,Neutrophils,Periparturient diseases},
number = {1-2},
pages = {21--29},
pmid = {16740320},
title = {{Neutrophil function and energy status in Holstein cows with uterine health disorders}},
volume = {113},
year = {2006}
}
