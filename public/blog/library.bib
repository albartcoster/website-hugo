journal = {Journal of dairy science},
keywords = {Animal Nutritional Physiological Phenomena,Animals,Cattle,Cattle: growth {\&} development,Cattle: physiology,DNA,DNA: analysis,Diet,Female,Food Deprivation,Growth Hormone,Growth Hormone: blood,Insulin,Insulin: blood,Lactation,Lactation: physiology,Lipids,Lipids: analysis,Mammary Glands, Animal,Mammary Glands, Animal: chemistry,Pregnancy,Proteins,Proteins: analysis,RNA,RNA: analysis,Weight Gain},
