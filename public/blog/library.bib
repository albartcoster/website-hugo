file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Phuntsok et al. - 1998 - Biogenic amines in silage, apparent postruminal passage, and the relationship between biogenic amines and diges.pdf:pdf},
issn = {0022-0302},
journal = {Journal of dairy science},
keywords = {Abomasum,Abomasum: metabolism,Amino Acids,Amino Acids: metabolism,Animals,Biogenic Amines,Biogenic Amines: analysis,Cattle,Cattle: physiology,Digestion,Eating,Fatty Acids, Volatile,Fatty Acids, Volatile: metabolism,Gastrointestinal Motility,Hydrogen-Ion Concentration,Insulin,Insulin: blood,Male,Medicago sativa,Polyamines,Polyamines: administration {\&} dosage,Polyamines: analysis,Quaternary Ammonium Compounds,Quaternary Ammonium Compounds: analysis,Rumen,Rumen: metabolism,Silage,Silage: analysis,Zea mays},
