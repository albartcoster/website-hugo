@article{Li2012,
abstract = {The effects of a grain-based subacute ruminal acidosis (SARA) challenge (GBSC) and an alfalfa-pellet SARA challenge (APSC) on fermentation and endotoxins in the rumen and in the cecum, as well as on endotoxins in peripheral blood, were determined. Six nonlactating Holstein cows with cannulas in the rumen and cecum were used in the study. A 3×3 Latin square arrangement of treatments with 4-wk experimental periods was adopted. During the first 3 wk of each experimental period, all cows received a diet containing 70{\%} forages [dry matter (DM) basis]. In wk 4 of each period, cows received 1 of the following 3 diets: the 70{\%} forage diet fed during wk 1 to 3 (control), a diet in which 34{\%} of the dietary DM was replaced with grain pellets made of 50{\%} ground wheat and 50{\%} ground barely (GBSC), or a diet in which 37{\%} of dietary DM was replaced with pellets of ground alfalfa (APSC). Rumen pH was monitored continuously using indwelling pH probes, and rumen fluid, blood, cecal digesta, and fecal grab samples were collected immediately before feed delivery at 0900 h and at 6 h after feed delivery on d 3 and 5 of wk 4. The time for which rumen pH was below 5.6 was 56.4, 225.2, and 298.8 min/d for the control, APSC, and GBSC treatments, respectively. Compared with the control, SARA challenges resulted in similar reductions in cecal digesta pH, which were 7.07, 6.86, and 6.79 for the control, APSC, and GBSC treatments, respectively. Compared with the control, only GBSC increased starch content in cecal digesta, which averaged 2.8, 2.6, and 7.4{\%} of DM for the control, APSC, and GBSC, respectively. Free lipopolysaccharide endotoxin (LPS) concentration in rumen fluid increased from 10,405 endotoxin units (EU)/mL in the control treatment to 30,715 and 168,391 EU/mL in APSC and GBSC, respectively. Additionally, GBSC increased the LPS concentration from 16,508 to 118,522 EU/g in wet cecal digesta, and from 12,832 to 93,154 EU/g in wet feces. The APSC treatment did not affect LPS concentrations in cecal digesta and feces. All concentrations of LPS in blood plasma were below the detection limit of {\textgreater}0.05 EU/mL of the technique used. Despite the absence of LPS in blood, only GBSC increased the concentration of LPS-binding protein in blood plasma, which averaged, 8.9, 9.5, and 12.1mg/L for the control, APSC, and GBSC treatments, respectively. This suggests that GBSC caused translocation of LPS from the digestive tract but that LPS was detoxified before entering the peripheral blood circulation. The higher LPS concentration in cecal digesta in the GBSC compared with the APSC suggests a higher risk of LPS translocation in the large intestine in GBSC than in APSC.},
author = {Li, S. and Khafipour, E. and Krause, D.O. and Kroeker, A. and Rodriguez-Lecompte, J.C. and Gozho, G.N. and Plaizier, J.C.},
doi = {10.3168/jds.2011-4447},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Li et al. - 2012 - Effects of subacute ruminal acidosis challenges on fermentation and endotoxins in the rumen and hindgut of dairy cows.pdf:pdf},
isbn = {0022-0302},
issn = {00220302},
journal = {Journal of Dairy Science},
keywords = {cecum,endotoxin,fermentation,subacute ruminal acidosis},
number = {1},
pages = {294--303},
pmid = {22192209},
title = {{Effects of subacute ruminal acidosis challenges on fermentation and endotoxins in the rumen and hindgut of dairy cows}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022030211007016},
volume = {95},
year = {2012}
}
@techreport{nizo2011,
abstract = {In Nederland vormt kuilvoer een belangrijke component in het dieet van melkvee. In perioden dat de dieren niet worden beweid is circa 70{\%} van de droge stof in het voer van melkvee afkomstig uit kuilvoer. Dit betreft voornamelijk graskuilen en snijma{\"{i}}skuilen. In ongeveer 10{\%} van de graskuilen en 1{\%} van de snijma{\"{i}}skuilen worden additieven toegepast met als doel de fermentatie te stimuleren en/of het a{\"{e}}roob bederf te reduceren. Naar aanleiding van gewijzigde EU wetgeving op het gebied van additieven voor kuilvoer zijn in deze deskstudie risico's van deze additieven voor de kwaliteit en veiligheid van melk ge{\"{i}}nventariseerd. In 2010 is vernieuwde wetgeving van kracht geworden voor additieven in diervoeder (European Parliament and Council Regulation (EC) No 1831/2003). Hierin is opgenomen dat additieven voor diervoeder alleen mogen worden toegepast, wanneer hiervoor vergunning verleend is. Ook zijn richtlijnen voor verpakking en etikettering vastgelegd. Vergunningen worden voor een periode van 10 jaar verleend. Kuiltoevoegmiddelen vallen onder deze regeling en zijn gedefinieerd als substanties, inclusief enzymen of micro-organismen, bedoeld om te worden opgenomen in het diervoeder om de kwaliteit van kuilvoer te verbeteren. Er is een register op internet waarin de toegestane middelen zijn gepubliceerd. Kuiltoevoegmiddelen stimuleren een goede ontwikkeling van de kuil waarbij suikers afkomstig uit het gewas door aanwezige melkzuurbacteri{\"{e}}n worden gefermenteerd tot organische zuren. In combinatie met het lage zuurstof gehalte in de kuil zorgt dit voor een goede conservering en daarmee een lange bewaartijd. Diverse soorten melkzuurbacteri{\"{e}}n, zoals Lactobacillus plantarum, Lactobacillus brevis, Lactobacillus buchneri, Lactobacillus casei, Pediococcus acidilactici, Pediococcus pentosaceus, kunnen worden toegepast om de fermentatie te stimuleren. Enzymen, zoals (hemi-)cellulases, xylanases, amylases en glucanases, worden toegepast om de beschikbaarheid van fermenteerbare suikers te vergroten of NIZO-Rapport E 2011/231 pag. 3 van 18 het vezelgehalte van de kuil te verlagen. Diverse organische (mierenzuur, benzo{\"{e}}zuur), maar ook anorganische zuren (zoutzuur, zwavelzuur) kunnen worden toegevoegd voor conservering. Er zijn geen risico's te verwachten van microbiologische en chemische kuiltoevoegmiddelen voor de kwaliteit en veiligheid van melk.},
author = {van Hekezen, R. and Driehuis, F.},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/van Hekezen, Driehuis - 2011 - NIZO rapport Wetgeving, gebruik en risico's van kuiltoevoegmiddelen.pdf:pdf},
keywords = {inkuilmiddelen,silage inoculant},
number = {December},
title = {{NIZO rapport: Wetgeving, gebruik en risico's van kuiltoevoegmiddelen}},
year = {2011}
}
