doi = {10.3168/jds.2013-7767},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Vargas-Rodriguez et al. - 2014 - Effects of supplemental chromium propionate and rumen-protected amino acids on productivity, diet diges.pdf:pdf},
issn = {1525-3198},
