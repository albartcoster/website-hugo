journal = {Journal of dairy science},
keywords = {Animals,Antigens, CD18,Antigens, CD18: blood,Cattle,Cattle: physiology,Cell Adhesion,Cell Adhesion Molecules,Cell Adhesion Molecules: blood,Female,Flow Cytometry,Immune Tolerance,L-Selectin,L-Selectin: blood,Leukocyte Count,Mammary Glands, Animal,Mammary Glands, Animal: physiology,Mammary Glands, Animal: surgery,Mastectomy,Mastectomy: veterinary,Neutrophils,Neutrophils: physiology,Peroxidase,Peroxidase: metabolism,Pregnancy},
number = {11},
