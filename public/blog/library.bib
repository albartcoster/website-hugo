journal = {The professional animal scientist},
keywords = {Calcium,Energy,Oral Drenching,Periparturient Problems.,Transition},
pages = {115--122},
title = {{Case Study : Evaluation of Calcium Propionate and Propylene Glycol Administered into the Esophagus of Dairy Cattle at Calving}},
volume = {17},
year = {2001}
}
