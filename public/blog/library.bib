@unpublished{hanigan2010,
abstract = {The lactating dairy cow is fairly inefficient at converting dietary nitrogen to milk protein. When cows are fed to National Research Council (NRC, 2001) requirements, they convert approximately 25 percent of dietary nitrogen to milk protein. There may be some inherent limitations to the efficiency that can be achieved by a ruminant; however, examination of the NRC model with respect to available data suggests several areas where requirement or supply predictions could be improved. Ruminally degradable protein requirements appear to be too high. Reducing these requirements would allow reductions in dietary protein and improved animal efficiency. Amino acid (AA) flow to the small intestine appears to be predicted with good accuracy, provided diets are within current NRC requirements for ruminally degradable protein. Absorption of AA from the gut lumen is not constant across AA within an ingredient or across ingredients as assumed by NRC. Thus, absorbed AA may be predicted with bias. The gut tissues as represented in the portal-drained viscera and the liver remove approximately two-thirds of the available AA on a daily basis, and this removal is dependent on supply. Variable use by these maintenance tissues violates the NRC assumption of fixed maintenance use and introduces a significant bias in our current predictions. Mammary AA removal is highly regulated and counters supply (i.e., as supply declines transport activity increases and vice versa). Variable transport activity with respect to AA supply violates the NRC assumption of fixed conversion efficiencies for absorbed AA and introduces bias into predictions. Thus, our ability to successfully balance diets for adequate AA while minimizing dietary nitrogen inputs is currently hampered by a lack of data with respect to AA digestibility in the small intestine and poor representations of postabsorptive use of AA. Amino acid digestibility coefficients for individual ingredients are needed to address the first challenge, and an alternate representation of postabsorptive use and recycling of AA is required to address the second challenge.},
author = {Hanigan, M D and Cyriac, J.},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Hanigan, Cyriac - 2010 - Current Status of Amino Acid Requirement Models for Lactating Dairy Cows.pdf:pdf},
institution = {eXtension},
keywords = {Amino Acids balance,dairy cow,protein metabolism},
title = {{Current Status of Amino Acid Requirement Models for Lactating Dairy Cows}},
url = {http://www.extension.org/pages/11231/current-status-of-amino-acid-requirement-models-for-lactating-dairy-cows},
year = {2010}
}
