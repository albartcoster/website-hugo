issn = {0002-9165},
journal = {The American journal of clinical nutrition},
keywords = {Animal Feed,Animals,Cattle,Food, Fortified,Humans,Linoleic Acids, Conjugated,Linoleic Acids, Conjugated: analysis,Meat,Meat: analysis,Milk,Milk: chemistry,Nutritional Physiological Phenomena},
