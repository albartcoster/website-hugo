file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Rulquin et al. - 1993 - Milk production and composition as a function of postruminal lysine and methionine supply A nutrient response ap.pdf:pdf},
isbn = {0301-6226},
journal = {Livestock Production Science},
keywords = {00,0301-6226,06,1993 elsevier science publishers,35590,93,a,all rights reserved,b,correspondence to,dairy cow,france,h,i,la vache laiti6re,limiting amino acid,lysine,methionine,milk protein,n,r,rulquin,saint gilles,station de recherches sur,v},
number = {1-2},
pages = {69--90},
title = {{Milk production and composition as a function of postruminal lysine and methionine supply: A nutrient response approach}},
