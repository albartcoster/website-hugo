file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Weiss et al. - 1990 - Effect of duration of supplementation of selenium and vitamin E on periparturient dairy cows.pdf:pdf},
issn = {0022-0302},
journal = {Journal of dairy science},
