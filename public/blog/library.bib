journal = {The Journal of dairy research},
keywords = {Administration, Oral,Animals,Apoptosis,Apoptosis: drug effects,Cattle,Cell Count,Cell Count: veterinary,Diet,Diet: veterinary,Dietary Supplements,Fatty Acids,Fatty Acids: pharmacology,Female,Flow Cytometry,Flow Cytometry: veterinary,Lactation,Lactation: drug effects,Lactation: immunology,Mastitis, Bovine,Mastitis, Bovine: immunology,Mastitis, Bovine: prevention {\&} control,Milk,Milk: cytology,Neutrophils,Neutrophils: drug effects,Neutrophils: physiology,Peripartum Period,Peripartum Period: drug effects,Peripartum Period: immunology},
