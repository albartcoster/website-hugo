publisher = {Elsevier},
title = {{Short communication: Effects of prill size of a palmitic acid-enriched fat supplement on the yield of milk and milk components, and nutrient digestibility of dairy cows.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28341047},
