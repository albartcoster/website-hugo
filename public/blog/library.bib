journal = {Journal of dairy science},
keywords = {Animals,Antioxidants,Antioxidants: adverse effects,Antioxidants: metabolism,Cattle,Cattle: blood,Cattle: physiology,Dietary Supplements,Dietary Supplements: adverse effects,Female,Free Radicals,Incidence,Lactation,Lactation: physiology,Mastitis, Bovine,Mastitis, Bovine: chemically induced,Mastitis, Bovine: epidemiology,Oxidative Stress,Oxidative Stress: drug effects,Postpartum Period,Retrospective Studies,Risk,Vitamin E,Vitamin E: adverse effects,Vitamin E: blood,Vitamins,Vitamins: adverse effects,Vitamins: blood},
