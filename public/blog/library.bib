doi = {10.3168/jds.S0022-0302(06)72082-3},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Schwab et al. - 2006 - Dietary Forage and Nonfiber Carbohydrate Contents Influence B-Vitamin Intake, Duodenal Flow, and Apparent Ruminal.pdf:pdf},
isbn = {1525-3198; 0022-0302},
issn = {00220302},
journal = {Journal of Dairy Science},
keywords = {B-vitamin,b-vitamin,duodenal,duodenal flow,lactating cow,ruminal synthesis},
number = {1},
