file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Beauchemin et al. - 2008 - Short communication salivary secretion during meals in lactating dairy cattle.pdf:pdf},
issn = {1525-3198},
journal = {Journal of dairy science},
keywords = {Animals,Cattle,Cattle: physiology,Feeding Behavior,Female,Food,Hordeum,Lactation,Lactation: physiology,Mastication,Medicago sativa,Saliva,Saliva: secretion,Silage,Time Factors},
