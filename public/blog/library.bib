doi = {10.3168/jds.S0022-0302(03)73806-5},
file = {:C$\backslash$:/Users/Gebruiker/Documents/Mendeley Desktop/Neylon, Kung - 2003 - Effects of cutting height and maturity on the nutritive value of corn silage for lactating cows.pdf:pdf},
