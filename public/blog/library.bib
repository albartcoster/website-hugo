keywords = {calcium supplementation,dairy cow,milk yield,reproduction},
pages = {1--20},
pmid = {27423945},
publisher = {American Dairy Science Association},
title = {{Effects of oral calcium supplementation on productive and reproductive performance in Holstein cows.}},
url = {http://www.journalofdairyscience.org/article/S0022030216304519/fulltext},
year = {2016}
}
